<table border = "1" cellpadding = "15">
           <tr>
               <th>Strain</th>
               <th>Phenotype</th>
               <th>THC %</th>
               <th>CBD %</th>
               <th>Price ($/lb)</th>
               <th>Clean Green Certified</th>
               <th>Grower</th>
               <th>Tools</th>
           </tr>
           {% if strains is not empty %}
               {% for strain in strains %}
               <tr>
                   <td>
                       {{ strain.getStrainName }}
                   </td>
                   <td>
                       {{ strain.getPheno }}
                   </td>
                   <td>
                       {{ strain.getThc }}
                   </td>
                   <td>
                       {{ strain.getCbd }}
                   </td>
                   <td>
                       {{ strain.getPrice }}
                   </td>
                   <td>
                       {% if strain.getCgc == 1 %}
                           &#10004;
                       {% else %}
                           &#x2718;
                       {% endif %}
                   </td>
                   <td>
                       {{ strain.findGrowerName(strain.getGrowersId) }}
                   </td>
                   <td>
                       <a href='/strain/{{strain.getId}}/edit_strain'>Edit</a>
                     
                       <a href='/strain/{{strain.getId}}/delete_strain'>Delete</a>
                   </td>
               </tr>
               {% endfor %}
           {% else %}
           <tr>
               <td colspan = "7">
                    No Strains Available
               </td>
           </tr>
           {% endif %}
       </table>
